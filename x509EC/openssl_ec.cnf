[ req ]
default_bits       = 256
prompt             = no
default_md         = sha256
req_extensions     = v3_req
distinguished_name = req_distinguished_name

[ req_distinguished_name ]
C  = GR
ST = GR
O  = UAegean
OU = UAegean
CN = uaegean.gr

[ v3_req ]
subjectAltName = @alt_names

[ alt_names ]
DNS.1 = dss.aegean.gr
DNS.2 = www.dss.aegean.gr
